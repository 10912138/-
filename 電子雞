import time
import random
from random import choice
local_time = time.localtime() 
timeString = time.strftime("%Y/%m/%d %H:%M", local_time)
x=0
class Cat():
    def __init__(self,sleep,eat,play,love,pee,fart,poo):
        self.Eat=eat
        self.Sleep=sleep
        self.Play=play
        self.Love=love
        self.Pee=pee
        self.Fart=fart
        self.Poo=poo
    def eat(self):
        if B.Eat<0:
            print('再不讓我吃阿!!腰腰死了齁')
            print('死亡時間:',timeString)
            while True:
                x+=1
        elif B.Eat<17:
            B.Eat+=15
            B.Sleep-=random.randrange(2,8)
            B.Play-=random.randrange(2,8)
